{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["framework", "laravel", "repository"],
    "authors": [
        {
            "name": "Anne Jan Brouwer",
            "email": "annejan@noprotocol.nl",
            "homepage": "https://annejan.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-gd": "*",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^5.5",
        "intervention/image": "^2.5",
        "laravel/framework": "6.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6",
        "sentry/sentry-laravel": "^1.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "codacy/coverage": "^1.4",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "codeception/module-laravel5": "^1.0",
        "doctrine/dbal": "^2.10",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.2",
        "nunomaduro/larastan": "^0.5",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8",
        "sempro/phpunit-pretty-print": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
