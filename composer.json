{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["micropython", "badge", "repository"],
    "authors": [
        {
            "name": "Anne Jan Brouwer",
            "email": "brouwer@annejan.com",
            "homepage": "https://annejan.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hatchery@badge.team",
        "isseus": "https://github.com/BadgeTeam/Hatchery/issues"
    },
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.5",
        "ext-gd": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "asbiin/laravel-webauthn": "dev-update-to-laravel-7",
        "bacon/bacon-qr-code": "^2.0",
        "bepsvpt/secure-headers": "^5.6",
        "czproject/git-php": "^3.17",
        "darkaonline/l5-swagger": "^7",
        "intervention/image": "^2.5",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6",
        "pragmarx/google2fa-laravel": "^1.3",
        "pragmarx/recovery": "^0.1.0",
        "predis/predis": "^1",
        "sentry/sentry-laravel": "^1.7",
        "spatie/laravel-sitemap": "^5.7",
        "symfony/yaml": "^5",
        "verschuur/laravel-robotstxt": "^3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.*",
        "codacy/coverage": "^1.4",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.1",
        "codeception/module-laravel5": "^1.0",
        "composer/composer": "^1.10@dev",
        "doctrine/dbal": "^2.10",
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.3",
        "nunomaduro/larastan": "^0.5.2",
        "phan/phan": "^2.5",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "sempro/phpunit-pretty-print": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            ""
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
