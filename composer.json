{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["framework", "laravel", "repository"],
    "authors": [
        {
            "name": "Anne Jan Brouwer",
            "email": "annejan@noprotocol.nl",
            "homepage": "https://annejan.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "darkaonline/l5-swagger": "^6.0",
        "filp/whoops": "~2.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^6.0",
        "sentry/sentry-laravel": "^1.0.2",
        "bepsvpt/secure-headers": "^5.5.0",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "codeception/codeception": "^3.1",
        "doctrine/dbal": "^2.9",
        "fzaninotto/faker": "~1.8",
        "nunomaduro/larastan": "^0.4",
        "phpunit/phpunit": "^8",
        "barryvdh/laravel-ide-helper": "^2.6",
        "codacy/coverage": "^1.4.2",
        "mockery/mockery": "^1.2.3",
        "phpstan/phpstan": "^0.11.4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
