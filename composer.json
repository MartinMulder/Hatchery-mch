{
    "name": "badgeteam/hatchery",
    "description": "Hatchery micropython app repository . .",
    "keywords": ["framework", "laravel", "repository"],
    "author": "Anne Jan Brouwer <annejan@noprotocol.nl>",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "bepsvpt/secure-headers": "^3.0",
        "doctrine/dbal": "^2.5",
        "filp/whoops": "~2.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.8",
        "sentry/sentry-laravel": "^1.0.2"
    },
    "require-dev": {
        "codacy/coverage": "dev-master",
        "sempro/phpunit-pretty-print": "^1.0",
        "fzaninotto/faker": "~1.8",
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
